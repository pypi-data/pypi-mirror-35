{
	"version": 1,
	"disable_existing_loggers": false,
	"formatters": {
		"file": {
			"format": "%(asctime)s %(processName)-15s %(threadName)-20s  %(name)-40s %(funcName)-20s %(lineno)4d %(levelname)-8s | %(message)s"
		},
		"console": {
			"format": "%(levelname)-8s | %(message)s"
		}
	},
	"handlers": {
		"console_handler": {
			"class": "logging.StreamHandler",
			"formatter": "console",
			"level": "INFO"
		},
		"error_file_handler": {
			"class": "logging.handlers.RotatingFileHandler",
			"level": "ERROR",
			"formatter": "file",
			"filename": null,
			"maxBytes": 10485760,
			"backupCount": 20,
			"encoding": "utf8"
		},
		"critical_file_handler": {
			"class": "logging.handlers.RotatingFileHandler",
			"level": "CRITICAL",
			"formatter": "file",
			"filename": null,
			"maxBytes": 10485760,
			"backupCount": 20,
			"encoding": "utf8"
		}
	},
	"root": {
		"level": 1,
		"handlers": [
			"console_handler",
			"error_file_handler",
			"critical_file_handler"
		],
		"propogate": true
	},
	"loggers": {
		"peewee": {
			"level": "DEBUG",
			"propogate": true
		}
	}
}
