    +-org.jenkins-ci.main:jenkins-war:2.89
      +-org.jenkins-ci.main:jenkins-core:2.89
        +-org.acegisecurity:acegi-security:1.0.7
          +-commons-collections:commons-collections:3.1
and
+-io.jenkins:jenkinsfile-runner:1.0-SNAPSHOT
  +-io.jenkins:jenkinsfile-runner-setup:1.0-SNAPSHOT
    +-org.jenkins-ci.main:jenkins-war:2.89
      +-org.jenkins-ci.main:jenkins-core:2.89
        +-org.kohsuke.stapler:stapler-groovy:1.253
          +-org.kohsuke.stapler:stapler-jelly:1.253
            +-commons-collections:commons-collections:3.2.2
and
+-io.jenkins:jenkinsfile-runner:1.0-SNAPSHOT
  +-io.jenkins:jenkinsfile-runner-setup:1.0-SNAPSHOT
    +-org.jenkins-ci.main:jenkins-war:2.89
      +-org.jenkins-ci.main:jenkins-core:2.89
        +-org.kohsuke.stapler:stapler-groovy:1.253
          +-org.kohsuke.stapler:stapler-jelly:1.253
