#!/Users/greg/code/scratch/env/py36/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'clowdr==0.0.13rc0','console_scripts','clowdr'
__requires__ = 'clowdr==0.0.13rc0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('clowdr==0.0.13rc0', 'console_scripts', 'clowdr')()
    )
