<form col="2" cursor="fintspin">
    <group id="grp1" col="2">
        <label id="lab1" xalign="0.5" colspan="2"
            string="Load bank statement for the following bank account"/>
        <label name="accountnum"/>
        <field name="accountnum"/>
        <label name="bankname"/>
        <field name="bankname"/>
        <label name="last_date"/>
        <field name="last_date"/>
        <label name="last_balance"/>
        <field name="last_balance"/>
    </group>
    <group id="grp2" col="2">
        <label id="lab2" xalign="0.5" colspan="2"
            string="Parameters for loading the bank statement"/>
        <label name="datestart"/>
        <field name="datestart"/>
        <label name="dateend"/>
        <field name="dateend"/>
        <label name="journal"/>
        <field name="journal"/>
        <label id="lab1" xalign="0.0" colspan="2"
            string="Enter the PIN for the account:"/>
        <label name="fintspin"/>
        <field name="fintspin" widget="password"/>
    </group>
    <group id="grp3" col="2">
        <label name="skipknownlines"/>
        <field name="skipknownlines"/>
        <label name="linkinvoice"/>
        <field name="linkinvoice"/>
    </group>
    <field name="accountnumbers"/>
    <field name="currency_digits"/>
    <field name="statements"/>
</form>
