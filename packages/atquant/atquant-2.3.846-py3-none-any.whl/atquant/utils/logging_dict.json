{
	"version": 1,
	"formatters": {
		"simple": {
			"format": "%(asctime)s FuctionName::%(funcName)s - %(lineno)s - %(name)s :: %(levelname)s - %(message)s"
		}
	},
	"handlers": {
		"console_handler": {
			"class": "logging.StreamHandler",
			"level": "DEBUG",
			"formatter": "simple",
			"stream": "ext://sys.stdout"
		},
		"sys_handler":{
			"class": "logging.handlers.RotatingFileHandler",
			"level":"DEBUG",
			"formatter": "simple",
			"maxBytes":10485760,
			"backupCount":10
		},
		"usr_handler":{
			"class": "logging.handlers.RotatingFileHandler",
			"level":"DEBUG",
			"formatter": "simple",
			"maxBytes":5242880,
			"backupCount":10
		}
	},
	"loggers": {
		"atquant": {
			"level": "DEBUG",
			"handlers": [
				"sys_handler"
			],
			"propagate": 0
		},
		"userlog": {
			"level": "DEBUG",
			"handlers": [
				"usr_handler"
			],
			"propagate": 0
		}
	}
}